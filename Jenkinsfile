pipeline {
  agent any
  stages {
    stage('Stage1') {
      steps {
        sh 'echo Hello World'
      }
    }
    stage('Stage2') {
      steps {
        echo 'I am Stazge2'
      }
    }
     stage('Stage3') {
      steps {
        echo 'I am Stazge3'
      }
    }
         stage('Stage4') {
      steps {
        echo 'I am Stazge4'
      }
    }
  }
}
